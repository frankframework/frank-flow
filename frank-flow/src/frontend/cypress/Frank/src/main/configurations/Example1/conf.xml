<Configuration name="${displayName}">
	<Adapter name="confAdapter">
		<Receiver name="confReceiver">
			<JavaListener name="confListener" serviceName="confService" />
		</Receiver>
		<Pipeline firstPipe="confPipe">
			<FixedResultPipe name="confPipe" returnString="Hello World">
				<Forward name="success" path="EXIT"/>
			</FixedResultPipe>
			<Exit path="EXIT" state="success"/>
		</Pipeline>
	</Adapter>
</Configuration>